{
	"name": "rakkas-scratchpad",
	"private": true,
	"version": "0.2.0",
	"description": "Rakkas TypeScript demo project",
	"scripts": {
		"dev": "rakkas dev",
		"build": "rakkas build",
		"start": "rakkas-node",
		"e2e": "cypress run",
		"e2e:dev": "start-server-and-test 'pnpm dev' :3000/api/ping 'pnpm e2e'",
		"e2e:prod": "start-server-and-test 'pnpm start' :3000/api/ping 'pnpm e2e'",
		"test": "jest --passWithNoTests",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint src",
		"lint:css": "stylelint \"src/**/*.css\"",
		"format": "prettier . --write"
	},
	"dependencies": {
		"@rakkasjs/runner-node": "workspace:*",
		"rakkasjs": "workspace:*",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.0.9"
	},
	"devDependencies": {
		"@rakkasjs/cli": "workspace:*",
		"@testing-library/jest-dom": "^5.14.1",
		"@testing-library/react": "^12.0.0",
		"@types/jest": "^26.0.24",
		"@types/react": "^17.0.14",
		"@types/react-dom": "^17.0.9",
		"@types/testing-library__jest-dom": "^5.14.0",
		"@typescript-eslint/eslint-plugin": "^4.28.2",
		"@typescript-eslint/parser": "^4.28.2",
		"cypress": "^7.7.0",
		"esbuild": "^0.12.15",
		"esbuild-jest": "^0.5.0",
		"eslint": "^7.30.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-react": "^7.24.0",
		"eslint-plugin-react-hooks": "^4.2.0",
		"jest": "^27.0.6",
		"jest-css-modules-transform": "^4.3.0",
		"prettier": "^2.3.2",
		"start-server-and-test": "^1.12.6",
		"stylelint": "^13.13.1",
		"stylelint-config-css-modules": "^2.2.0",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-standard": "^22.0.0",
		"stylelint-prettier": "^1.2.0",
		"typescript": "^4.3.5"
	}
}
