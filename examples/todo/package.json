{
	"name": "@rakkasjs/example-todo",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "rakkas",
		"build": "rakkas build",
		"start": "node dist/server",
		"format": "prettier --write --ignore-unknown src",
		"test": "npm run test:typecheck && npm run test:format && npm run test:lint && npm run test:unit",
