	},
	"devDependencies": {
		"@rakkasjs/eslint-config": "0.7.0-next.8",
		"@types/react": "^18.0.31",
		"@types/react-dom": "^18.0.11",
		"eslint": "^8.37.0",
		"prettier": "^2.8.7",
		"rakkasjs": "0.7.0-next.8",
		"typescript": "^5.0.2",
		"vite": "^4.2.1",
		"vite-tsconfig-paths": "^4.0.7",
		"vitest": "^0.29.8"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
