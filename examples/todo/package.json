		"eslint": "^8.44.0",
		"prettier": "^2.8.8",
		"rakkasjs": "0.7.0-next.12",
		"typescript": "^5.1.6",
		"vite": "^4.3.9",
		"vite-tsconfig-paths": "^4.2.0",
		"vitest": "^0.32.2"
	},
	"dependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
