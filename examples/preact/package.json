{
	"name": "@rakkasjs/example-preact",
	"type": "module",
	"private": true,
	"scripts": {
		"dev": "rakkas",
		"build": "rakkas build",
		"start": "node dist/server",
		"format": "prettier --write --ignore-unknown src",
		"test": "npm run test:typecheck && npm run test:format && npm run test:unit",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:format": "prettier --check --ignore-unknown src",
		"test:unit": "vitest run"
	},
	"devDependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.17",
		"prettier": "^2.7.1",
		"rakkasjs": "0.6.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"typescript": "^4.8.2",
		"vite": "^3.1.0",
		"vite-tsconfig-paths": "^3.5.0",
		"vitest": "^0.23.1"
	},
	"dependencies": {
		"preact": "^10.11.0",
		"preact-render-to-string": "^5.2.4",
		"preact-ssr-prepass": "^1.2.0"
	}
}
