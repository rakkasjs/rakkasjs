{
	"name": "@rakkasjs/testbed-core",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite serve --port 3000 --strictPort",
		"build": "vavite",
		"start": "node dist/server",
		"ci": "vitest run --reporter=verbose --no-threads",
		"ci:local": "TEST_HOST=http://localhost:3000 pnpm run ci",
		"test": "pnpm test:typecheck && pnpm test:lint",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^2.1.3",
		"@rakkasjs/core": "workspace:*",
		"@rakkasjs/react": "workspace:*",
		"@types/ps-tree": "^1.1.2",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"cross-env": "^7.0.3",
		"eslint": "^8.32.0",
		"node-fetch": "^3.3.0",
		"ps-tree": "^1.2.0",
		"puppeteer": "^19.5.2",
		"typescript": "^4.9.4",
		"vavite": "^1.5.3",
		"vite": "^4.0.3"
	},
	"dependencies": {
		"@hattip/adapter-node": "^0.0.27",
		"@hattip/compose": "^0.0.27",
		"@hattip/core": "^0.0.27",
		"cheerio": "^1.0.0-rc.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
