{
	"name": "@rakkasjs/testbed-kitchen-sink",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "rakkas build",
		"build:cfw": "cross-env RAKKAS_TARGET=cloudflare-workers rakkas build",
		"start": "node dist/server",
		"ci": "vitest run --reporter=verbose --no-threads",
		"ci:local": "TEST_HOST=http://localhost:3000 pnpm run ci",
		"test": "pnpm test:typecheck && pnpm test:lint",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^2.0.0",
		"@types/ps-tree": "^1.1.2",
		"cross-env": "^7.0.3",
		"eslint": "^8.18.0",
		"miniflare": "^2.6.0",
		"node-fetch": "^3.2.6",
		"ps-tree": "^1.2.0",
		"puppeteer": "^15.2.0",
		"rakkasjs": "workspace:*",
		"typescript": "^4.7.4",
		"vite": "^3.0.0-beta.9",
		"wrangler": "^2.0.17"
	},
	"dependencies": {
		"@hattip/adapter-cloudflare-workers": "^0.0.8",
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"cheerio": "1.0.0-rc.12",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
