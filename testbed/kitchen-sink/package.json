{
  "name": "@rakkasjs/testbed-kitchen-sink",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "rakkas --port 3000",
    "build": "rakkas build",
    "build:cfw": "cross-env RAKKAS_TARGET=cloudflare-workers-node-compat rakkas build",
    "build:netlify": "rimraf .netlify/edge-functions-dist && cross-env RAKKAS_TARGET=netlify rakkas build",
    "build:netlify-edge": "cross-env RAKKAS_TARGET=netlify-edge rakkas build",
    "build:deno": "cross-env RAKKAS_TARGET=deno rakkas build",
    "build:vercel": "cross-env RAKKAS_TARGET=vercel rakkas build",
    "build:vercel-edge": "cross-env RAKKAS_TARGET=vercel-edge rakkas build",
    "start": "node dist/server/index.js",
    "ci": "vitest run --reporter=verbose",
    "ci:local": "TEST_HOST=http://localhost:3000 pnpm run ci",
    "test": "pnpm run /^test:/",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
  },
  "devDependencies": {
    "@cyco130/eslint-config": "^3.9.5",
    "@hattip/adapter-bun": "0.0.48",
    "@hattip/adapter-lagon": "0.0.48",
    "@hattip/adapter-netlify-edge": "0.0.48",
    "@hattip/adapter-netlify-functions": "0.0.48",
    "@rakkasjs/css": "workspace:*",
    "@types/ps-tree": "^1.1.6",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "node-fetch": "^3.3.2",
    "ps-tree": "^1.2.0",
    "puppeteer": "^23.3.0",
    "rakkasjs": "workspace:*",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-inspect": "^0.8.7",
    "wrangler": "^3.75.0"
  },
  "dependencies": {
    "@hattip/adapter-cloudflare-workers": "0.0.48",
    "@hattip/adapter-deno": "0.0.48",
    "@hattip/adapter-vercel-edge": "0.0.48",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "cheerio": "^1.0.0",
    "netlify-cli": "^17.35.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "web-streams-polyfill": "^4.0.0"
  }
}
