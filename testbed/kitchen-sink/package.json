{
	"name": "@rakkasjs/testbed-kitchen-sink",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "rakkas build",
		"start": "node dist/server",
		"ci": "vitest run",
		"ci:local": "TEST_HOST=http://localhost:3000 vitest run",
		"test": "pnpm test:typecheck && pnpm test:lint",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^2.0.0",
		"eslint": "^8.18.0",
		"kill-port": "^2.0.1",
		"node-fetch": "^3.2.6",
		"puppeteer": "^14.4.1",
		"rakkasjs": "workspace:*",
		"typescript": "^4.7.4",
		"vite": "^3.0.0-beta.1"
	},
	"dependencies": {
		"@types/react": "^18.0.14",
		"@types/react-dom": "^18.0.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	}
}
