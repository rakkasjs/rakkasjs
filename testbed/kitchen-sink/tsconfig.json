{
	"compilerOptions": {
		"target": "es2020",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"outDir": "dist",
		"checkJs": true
	},
	"exclude": ["dist", "netlify", ".vercel", ".netlify"]
}
