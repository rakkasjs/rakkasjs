{
	"private": true,
	"name": "rakkas-styled-components-testbed",
	"version": "0.5.0-canary.12",
	"description": "Rakkas.js style-components example project",
	"scripts": {
		"dev": "rakkas dev --open",
		"build": "rakkas build",
		"start": "node dist/server",
		"check": "run-p test typecheck",
		"test": "rakkas with-port \"start-server-and-test 'pnpm start' http-get://\\$HOST:\\$PORT 'cypress run'\"",
		"typecheck": "tsc -p tsconfig.json --noEmit"
	},
	"dependencies": {
		"rakkasjs": "workspace:*",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-helmet-async": "^1.1.2",
		"styled-components": "^5.3.1"
	},
	"devDependencies": {
		"@rakkasjs/cli": "workspace:*",
		"@types/react": "^17.0.27",
		"@types/react-dom": "^17.0.9",
		"@types/styled-components": "^5.1.15",
		"babel-plugin-styled-components": "^1.13.2",
		"cypress": "^8.6.0",
		"npm-run-all": "^4.1.5",
		"start-server-and-test": "^1.14.0",
		"typescript": "^4.4.3"
	}
}
