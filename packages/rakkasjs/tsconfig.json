{
	"compilerOptions": {
		"target": "es2021",
		"module": "ESNext",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true,
		"moduleResolution": "Node",
		"resolveJsonModule": true,
		"jsx": "react",
		"verbatimModuleSyntax": true,
		"outDir": "dist"
	},
	"exclude": ["dist", "*.d.ts"]
}
