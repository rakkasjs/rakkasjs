{
	"name": "rakkasjs",
	"version": "0.6.0-next.6",
	"type": "module",
	"bin": {
		"rakkas": "./cli.js",
		"rakkasjs": "./cli.js"
	},
	"files": [
		"dist",
		"cli.js",
		"*.d.ts",
		"vite-license.md",
		"react-helmet-async-license.txt",
		"json-s-license.md"
	],
	"exports": {
		".": {
			"browser": "./dist/client.js",
			"default": "./dist/server.js"
		},
		"./node-adapter": "./dist/node-adapter.js",
		"./vite-plugin": "./dist/vite-plugin.js"
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit && pnpm test:package",
		"test:unit": "vitest run --reporter=verbose",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
		"test:package": "publint"
	},
	"description": "Lightning fast Next.js alternative powered by Vite",
	"license": "MIT",
	"repository": "https://github.com/rakkasjs/rakkasjs",
	"homepage": "https://rakkasjs.org",
	"keywords": [
		"react",
		"vite",
		"nextjs",
		"ssr"
	],
	"peerDependencies": {
		"react": "^18",
		"react-dom": "^18",
		"vite": "3"
	},
	"devDependencies": {
		"@brillout/json-s": "^0.4.6",
		"@cyco130/eslint-config": "^2.0.0",
		"@types/babel__core": "^7.1.19",
		"@types/micromatch": "^4.0.2",
		"@types/prettier": "^2.6.3",
		"eslint": "^8.19.0",
		"prettier": "^2.7.1",
		"publint": "^0.1.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-helmet-async": "^1.3.0",
		"tsup": "^6.1.3",
		"typescript": "^4.7.4",
		"vite": "^3.0.0"
	},
	"dependencies": {
		"@babel/core": "^7.18.5",
		"@babel/types": "^7.18.8",
		"@hattip/adapter-node": "^0.0.10",
		"@hattip/bundler-cloudflare-workers": "^0.0.10",
		"@hattip/bundler-deno": "^0.0.10",
		"@hattip/bundler-netlify": "^0.0.10",
		"@hattip/bundler-vercel": "^0.0.10",
		"@hattip/compose": "^0.0.10",
		"@hattip/core": "^0.0.10",
		"@hattip/polyfills": "^0.0.10",
		"@types/node": "^18.0.5",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"@vavite/connect": "^1.4.0",
		"@vavite/expose-vite-dev-server": "1.4.0",
		"@vavite/multibuild": "^1.4.0",
		"@vitejs/plugin-react": "^2.0.0",
		"cac": "^6.7.12",
		"cheerio": "1.0.0-rc.12",
		"devalue": "^2.0.1",
		"fast-glob": "^3.2.11",
		"micromatch": "^4.0.5",
		"picocolors": "^1.0.0",
		"react-error-boundary": "^3.1.4"
	}
}
