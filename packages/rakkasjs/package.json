{
  "name": "rakkasjs",
  "version": "0.7.0-next.48",
  "type": "module",
  "bin": {
    "rakkas": "./cli.js",
    "rakkasjs": "./cli.js"
  },
  "files": [
    "dist",
    "cli.js",
    "node-loader.mjs",
    "suppress-loader-warnings.cjs",
    "*.d.ts",
    "vite-license.md",
    "json-s-license.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/index.client.js",
      "default": "./dist/index.server.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "browser": "./dist/client.js",
      "default": "./dist/client-stub.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "browser": "./dist/server-stub.js",
      "default": "./dist/server.js"
    },
    "./node-adapter": "./dist/node-adapter.js",
    "./vite-plugin": "./dist/vite-plugin.js",
    "./node-loader": "./node-loader.mjs",
    "./suppress-loader-warnings": "./suppress-loader-warnings.cjs"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "test": "pnpm run /^test:/",
    "test:unit": "vitest run --reporter=verbose",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
    "test:package": "publint --strict"
  },
  "description": "Bleeding-edge React framework powered by Vite",
  "license": "MIT",
  "repository": "https://github.com/rakkasjs/rakkasjs",
  "homepage": "https://rakkasjs.org",
  "keywords": [
    "react",
    "vite",
    "nextjs",
    "ssr"
  ],
  "peerDependencies": {
    "react": "18",
    "react-dom": "18",
    "typescript": "5",
    "vite": "5"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    }
  },
  "devDependencies": {
    "@brillout/json-serializer": "^0.5.13",
    "@cyco130/eslint-config": "^3.9.4",
    "@types/babel__core": "^7.20.5",
    "@types/micromatch": "^4.0.9",
    "@types/prettier": "^3.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.3",
    "publint": "^0.2.9",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.13",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "vite": "^5.4.0"
  },
  "dependencies": {
    "@babel/core": "^7.25.2",
    "@babel/types": "^7.25.2",
    "@hattip/adapter-node": "0.0.47",
    "@hattip/bundler-cloudflare-workers": "0.0.47",
    "@hattip/bundler-deno": "0.0.47",
    "@hattip/bundler-netlify": "0.0.47",
    "@hattip/bundler-vercel": "0.0.47",
    "@hattip/compose": "0.0.47",
    "@hattip/core": "0.0.47",
    "@hattip/polyfills": "0.0.47",
    "@microsoft/fetch-event-source": "^2.0.1",
    "@types/node": "^22.2.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vavite/connect": "^4.1.2",
    "@vavite/expose-vite-dev-server": "4.1.2",
    "@vavite/multibuild": "^4.1.2",
    "@vavite/node-loader": "^4.1.2",
    "cac": "^6.7.14",
    "cheerio": "^1.0.0",
    "devalue": "^5.0.0",
    "es-module-lexer": "^1.5.4",
    "fast-glob": "^3.3.2",
    "magic-string": "^0.30.11",
    "micromatch": "^4.0.7",
    "picocolors": "^1.0.1",
    "sirv": "^2.0.4"
  }
}
