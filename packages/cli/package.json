{
	"name": "@rakkasjs/cli",
	"description": "Development tools for Rakkas.js",
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "babel src --watch --out-dir dist --extensions '.ts,.js,.tsx,.jsx'",
		"build": "babel src --out-dir dist --extensions '.ts,.js'",
		"precommit": "lint-staged --no-stash",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint --max-warnings=0 src"
	},
	"bin": {
		"rakkas": "dist/index.js"
	},
	"peerDependencies": {
		"@rakkasjs/core": "workspace:*"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.3",
		"@babel/core": "^7.14.3",
		"@babel/preset-env": "^7.14.4",
		"@babel/preset-react": "^7.13.13",
		"@babel/preset-typescript": "^7.13.0",
		"@rakkasjs/core": "workspace:*",
		"@types/node": "^15.6.1",
		"@typescript-eslint/eslint-plugin": "^4.25.0",
		"@typescript-eslint/parser": "^4.25.0",
		"eslint": "^7.27.0",
		"eslint-plugin-import": "^2.23.3",
		"eslint-plugin-only-warn": "^1.0.2",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.0",
		"typescript": "^4.3.2"
	},
	"dependencies": {
		"@vitejs/plugin-react-refresh": "^1.3.3",
		"commander": "^7.2.0",
		"fast-glob": "^3.2.5",
		"html-entities": "^2.3.2",
		"vite": "^2.3.4"
	}
}
