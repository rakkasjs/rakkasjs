{
	"name": "@rakkasjs/cli",
	"description": "Development tools for Rakkas.js",
	"author": "Fatih Ayg√ºn <cyco130@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "babel src --watch --out-dir dist --extensions '.ts,.js,.tsx,.jsx'",
		"build": "rimraf dist && babel src --out-dir dist --extensions '.ts,.js'",
		"precommit": "lint-staged --no-stash",
		"typecheck": "tsc -p tsconfig.json --noEmit",
		"lint:ts": "eslint --max-warnings=0 src"
	},
	"bin": {
		"rakkas": "dist/bin.js"
	},
	"module": "dist/index.js",
	"peerDependencies": {
		"@rakkasjs/core": "workspace:*",
		"react": "^17",
		"react-dom": "^17"
	},
	"devDependencies": {
		"@babel/cli": "^7.14.5",
		"@babel/core": "^7.14.6",
		"@babel/preset-env": "^7.14.5",
		"@babel/preset-react": "^7.14.5",
		"@babel/preset-typescript": "^7.14.5",
		"@rakkasjs/core": "workspace:*",
		"@types/node": "^15.12.2",
		"@types/picomatch": "^2.2.4",
		"@typescript-eslint/eslint-plugin": "^4.27.0",
		"@typescript-eslint/parser": "^4.27.0",
		"esbuild": "^0.12.9",
		"eslint": "^7.28.0",
		"eslint-plugin-import": "^2.23.4",
		"eslint-plugin-only-warn": "^1.0.2",
		"lint-staged": "^11.0.0",
		"prettier": "^2.3.1",
		"rimraf": "^3.0.2",
		"typescript": "^4.3.3"
	},
	"dependencies": {
		"@rakkasjs/runner-node": "workspace:*",
