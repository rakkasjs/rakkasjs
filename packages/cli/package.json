		"commander": "^7.2.0",
		"fast-glob": "^3.2.5",
		"html-entities": "^2.3.2",
		"node-fetch": "^2.6.1",
		"picomatch": "^2.3.0",
		"sirv": "^1.0.12",
		"vite": "^2.3.7",
		"vite-plugin-virtual": "^0.1.1"
	}
