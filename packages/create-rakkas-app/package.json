    "@types/inquirer": "^9.0.7",
    "@types/node": "^22.2.0",
    "detype": "^1.0.10",
    "eslint": "^8.57.0",
    "publint": "^0.2.9",
    "tsup": "^8.2.4",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@inquirer/prompts": "^5.3.8",
    "cac": "^6.7.14",
    "cpr": "^3.0.1",
    "picocolors": "^1.0.1"
  }
