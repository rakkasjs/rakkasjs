{
  "name": "create-rakkas-app",
  "version": "0.7.0-next.24",
  "type": "module",
  "files": [
    "dist",
    "templates"
  ],
  "bin": {
    "create-rakkas-app": "./index.js"
  },
  "scripts": {
    "build": "tsup && tsx scripts/create-templates.ts",
    "dev": "tsup --watch",
    "prepack": "rm -rf dist && pnpm build",
    "test": "pnpm run /^test:/",
    "test:typecheck": "tsc -p tsconfig.json --noEmit",
    "test:lint": "eslint . --max-warnings 0 --ignore-pattern dist --ignore-pattern templates",
    "test:package": "publint --strict"
  },
  "description": "Generate Rakkas application boilerplate",
  "license": "MIT",
  "repository": "https://github.com/rakkasjs/rakkasjs",
  "keywords": [
    "react",
    "vite",
    "rakkas"
  ],
  "devDependencies": {
    "@cyco130/eslint-config": "^3.5.0",
    "@types/inquirer": "^9.0.7",
    "@types/node": "^20.10.5",
    "detype": "^0.6.3",
    "eslint": "^8.56.0",
    "publint": "^0.2.7",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "cac": "^6.7.14",
    "cpr": "^3.0.1",
    "inquirer": "^9.2.12",
    "mkdirp": "^3.0.1",
    "picocolors": "^1.0.0",
    "rimraf": "^5.0.5"
  }
