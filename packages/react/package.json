{
	"name": "@rakkasjs/react",
	"version": "0.6.13",
	"type": "module",
	"files": [
		"dist"
	],
	"exports": {
		"./server": "./dist/server.js"
	},
	"typesVersions": {
		"*": {
			"*": [
				"dist/*.d.ts"
			]
		}
	},
	"scripts": {
		"build": "tsup",
		"dev": "tsup --watch",
		"prepack": "rm -rf dist && pnpm build",
		"test": "pnpm test:typecheck && pnpm test:lint && pnpm test:unit && pnpm test:package",
		"test:unit": "vitest run --reporter=verbose",
		"test:typecheck": "tsc -p tsconfig.json --noEmit",
		"test:lint": "eslint . --max-warnings 0 --ignore-pattern dist",
		"test:package": "publint"
	},
	"description": "React renderer for Rakkas",
	"license": "MIT",
	"repository": "https://github.com/rakkasjs/rakkasjs",
	"homepage": "https://rakkasjs.org",
	"keywords": [
		"react",
		"vite",
		"nextjs",
		"ssr"
	],
	"peerDependencies": {
		"react": "^18.2.0",
		"react-dom": "^18.2.0"
	},
	"devDependencies": {
		"@cyco130/eslint-config": "^2.1.3",
		"@types/micromatch": "^4.0.2",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"eslint": "^8.32.0",
		"prettier": "^2.8.3",
		"publint": "^0.1.8",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"tsup": "^6.5.0",
		"typescript": "^4.9.4",
		"vite": "^4.0.3"
	},
	"dependencies": {
		"@hattip/compose": "^0.0.27"
	}
}
